
public class Car {

	String name;
	int speed;
	static int numberOfCars;

	public Car() {
				name = "MyCar";
				speed = 0;
				numberOfCars++;
	}	

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}


	public int getSpeed() {
				return speed;
	}
	
}
