package Shape;

public class ShapeApp {

	public static void main(String[] args) {
		Point point1= new Point();
		point1.setX(2);
		point1.setY(5);

		Point point2= new Point();
		point2.setX(10);
		point2.setY(23);
		
		point1.show();
		point2.show();
		
		Point point3=new Point(50, 50);
		
		point3.show();
	}

}
